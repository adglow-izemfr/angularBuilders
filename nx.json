{
  "npmScope": "adglow-izemfr",
  "affected": {
    "defaultBase": "main"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "shared-data": {
      "tags": ["shared", "core"]
    },
    "shared-global": {
      "tags": ["shared", "core"]
    },
    "shared-ui": {
      "tags": ["shared", "ui"]
    },
    "www": {
      "tags": []
    },
    "www-e2e": {
      "tags": [],
      "implicitDependencies": ["www"]
    }
  }
}
